{
  "name": "pm2-logrotate",
  "version": "1.3.1",
  "description": "Module to rotate logs of every pm2 application",
  "main": "app.js",
  "dependencies": {
    "moment": "2.10.x",
    "pm2": "latest",
    "pmx": "latest",
    "rolex": "0.0.x"
  },
  "scripts": {
    "test": "mocha test/**/*.js"
  },
  "author": "Joni SHKURTI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pm2-hive/pm2-logrotate.git"
  },
  "bugs": {
    "url": "https://github.com/pm2-hive/pm2-logrotate/issues"
  },
  "homepage": "https://github.com/pm2-hive/pm2-logrotate",
  "license": "MIT",
  "apps": [
    {
      "name": "pm2-logrotate",
      "script": "app.js"
    }
  ],
  "config": {
    "interval_unit": "DD",
    "interval": "1",
    "max_size": "10M",
    "retain": "none"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3"
  }
}
